<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ferus.tigris.pingpong"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:allowBackup="true"
        android:theme="@style/AppTheme" android:logo="@drawable/logo" android:name="Pingpong">
        <activity
            android:name=".StartMenuActivity"
            android:label="@string/title_activity_freeman_field" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameFieldActivity"></activity>
    </application>

</manifest>